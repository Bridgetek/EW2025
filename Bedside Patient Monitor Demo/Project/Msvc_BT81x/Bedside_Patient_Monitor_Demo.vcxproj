    <LocalDebuggerWorkingDirectory>$(OutDir)</LocalDebuggerWorkingDirectory>
    <OutputDirectory>$(OutDir)</OutputDirectory>
    <IntermediateDirectory>$(OutDir)</IntermediateDirectory>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Convert $(Configuration) to uppercase -->
    <UppercaseConfiguration>$([System.String]::Copy($(Configuration)).ToUpperInvariant())</UppercaseConfiguration>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Add the DLL folder to the PATH -->
    <CommonPath>$(ProjectDir)..\..\Common</CommonPath>
    <EmulatorDllPath>$(CommonPath)\\eve_hal\\Bin\\Simulation\\$(Platform)</EmulatorDllPath>
    <SPIDllPath>$(CommonPath)\\eve_hal\\Bin\\MSVC\\$(Platform)</SPIDllPath>
    <Environment>PATH=$(EmulatorDllPath);$(SPIDllPath);$(PATH)</Environment>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <WarningLevel>Level3</WarningLevel>
      <Optimization>Disabled</Optimization>
      <!-- LCD: EVE_DISPLAY_WXGA-->
      <!-- EVE chip: EVE_GRAPHICS_BT817 or EVE_GRAPHICS_BT81* -->
      <!-- FT4222: EVE_PLATFORM_FT4222 -->
      <!-- MPSSE: EVE_PLATFORM_MPSSE -->
      <!-- EMU: EVE_PLATFORM_BT8XXEMU - not yet supported in BT820-->
      <PreprocessorDefinitions>$(UppercaseConfiguration);$(Platform);%(PreprocessorDefinitions);EVE_GRAPHICS_BT817;EVE_PLATFORM_BT8XXEMU;EVE_DISPLAY_WXGA;</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(CommonPath)\\application;$(CommonPath)\\eve_hal;$(CommonPath)\\eve_hal\\Hdr;..\..\Hdr\;</AdditionalIncludeDirectories>
      <AdditionalOptions>/VERBOSE /utf-8 </AdditionalOptions>
    </ClCompile>
    <Link>
      <SubSystem>Console</SubSystem>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <AdditionalLibraryDirectories>$(EmulatorDllPath);$(SPIDllPath);</AdditionalLibraryDirectories>
      <AdditionalDependencies>kernel32.lib;user32.lib;gdi32.lib;winspool.lib;comdlg32.lib;advapi32.lib;shell32.lib;ole32.lib;oleaut32.lib;uuid.lib;odbc32.lib;odbccp32.lib;%(AdditionalDependencies);bt8xxemu.lib;libMPSSE.lib;ftd2xx.lib;LibFT4222.lib</AdditionalDependencies>
    </Link>
    <PreBuildEvent>
      <Command>
        mkdir "$(OutDir)"
        echo D | xcopy /ecfhrsy  "$(EmulatorDllPath)\\*.*" "$(OutDir)" || exit 0
        echo D | xcopy /ecfhrsy  "$(SPIDllPath)\\*.*" "$(OutDir)" || exit 0
        echo F | xcopy /ecfhrsy  "$(ProjectDir)..\..\Test\Flash\*.bin" "$(OutDir)\__Flash.bin" || exit 0
      </Command>
    </PreBuildEvent>
  </ItemDefinitionGroup>
